{
    "name": "@boby/api-client",
    "version": "0.1.0",
    "private": true,
    "description": "Boby API Client - Type-safe API client for all backend calls",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "lint": "eslint src --ext .ts,.tsx",
        "typecheck": "tsc --noEmit",
        "clean": "rimraf dist"
    },
    "devDependencies": {
        "rimraf": "^5.0.5",
        "tsup": "^8.0.0",
        "typescript": "^5.3.0"
    }
}